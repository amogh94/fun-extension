{
    "name":"Playmarks",
    "version":"1.0",
    "manifest_version":2,


    "browser_action":{
        "default_popup":"home.html",
        "default_title":"Playmarks"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "history"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}